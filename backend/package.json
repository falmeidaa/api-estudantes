    "cors": "^2.8.5",
    "express": "^4.17.1",
    "js-brasil": "^2.5.1",
    "module-alias": "^2.2.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.29",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.11.10",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "faker": "^5.5.3",
    "jest": "^27.3.1",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^12.1.2",
    "pg-mem": "^2.1.9",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  }
